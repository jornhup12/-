/* pages/mine/mine.wxss */
.UCenter-bg {
  background-image: url(http://r.photo.store.qq.com/psc?/V10uH3z53mPeWp/u88agf.Ae1TGJZuaUYX5TSgz7jOp*lBHssUphWhU7rImjdJnxrWhnAacKsaLv9jCunS4Aw9RBz2wnyA7a5IKgmT.L7MrunYAHOFyXdaQx3s!/r);
  background-size: cover;
  height: 550rpx;
  display: flex;
  justify-content: center;
  padding-top: 40rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg image {
  width: 200rpx;
  height: 200rpx;
}

.UCenter-bg .gif-wave{
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  width: 750rpx;
  height: 300rpx;
}
