<!--pages/write/write.wxml-->
<view class="cu-bar bg-white solid-bottom ">
  <view class="action">
    <text class="cuIcon-title text-orange"></text> 常用表单
  </view>
  <view class="action">
  </view>
</view>
<scroll-view scroll-y class="scrollPage margin-top">
  <view class='nav-list'>
    <view  hover-class='none' data-index="{{index}}" class="nav-li bg-{{item.color}}" wx:for="{{elements}}" wx:key bindtap="navigateToPage">
      <view class="nav-title">{{item.title}}</view>
      <view class="nav-name">{{item.name}}</view>
      <text class='cuIcon-{{item.icon}}'></text>
    </view>
  </view>
  <view class='cu-tabbar-height'></view>
</scroll-view>